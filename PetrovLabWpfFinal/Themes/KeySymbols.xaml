<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:sys="clr-namespace:System;assembly=mscorlib">
    <sys:Char x:Key="addSymbol">+</sys:Char>
    <sys:Char x:Key="substractSymbol">-</sys:Char>
    <sys:Char x:Key="multipleSymbol">x</sys:Char>
    <sys:Char x:Key="divideSymbol">÷</sys:Char>
    <sys:Char x:Key="percentSymbol">%</sys:Char>
    <sys:Char x:Key="backSymbol">⇐</sys:Char>
    <sys:String x:Key="squareSymbol">x²</sys:String>
    <sys:String x:Key="rootSymbol">√x</sys:String>
    <sys:String x:Key="partWholeSymbol">1/x</sys:String>
</ResourceDictionary>
